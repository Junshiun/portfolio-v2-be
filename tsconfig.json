{
  "compilerOptions": {
    "module": "commonjs",
    "esModuleInterop": true,
    "target": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": "./",
    "paths": {
      "@controllers/*": ["src/controllers/*"],
      "@handlers/*": ["src/handlers/*"],
      "@services/*": ["src/services/*"],
      "@models/*": ["src/models/*"],
    }
  },
  "lib": ["es2015"]
}
